                        <div class="span1">
                        </div>
                        <div class="span3">
                            <p><b>Filter By Team</b></p>
                            <p>
                                <select data-bind="options: managePageTemplatesTeamList, optionsText: 'Title', optionsValue: 'Id', value: managePageTemplatesSelectedTeam" size="10"></select>
                            </p>
                        </div>
                        <div class="span3">
                            <p><b>Filter By Console Page</b></p>
                            <p>
                                <select data-bind="options: managePageTemplatesPageTemplateList, optionsText: 'Title', optionsValue: 'Id', value: selectedTemplatedPage" size="10"></select>
                            </p>

                        </div>
                        <div class="span4">
                            <div data-bind="visible: showPageStructureTemplateDiv">

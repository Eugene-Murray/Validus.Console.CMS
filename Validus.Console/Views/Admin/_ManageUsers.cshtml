                                                        <span data-bind="text: $data.TeamMemberships()[0].Team().Title"></span>
                                                    </td>
                                                    <!-- /ko -->
                                                    <!-- ko if:  !$data.TeamMemberships().length>0 -->
                                                    <td>
                                                        <input type="hidden" data-bind="value: $data.Id" />
                                                    </td>
                                                    <!-- /ko -->
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
